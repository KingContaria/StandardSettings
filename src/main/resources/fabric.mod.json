	"authors": [
		{
			"name": "contaria",
			"contact": {
				"homepage": "https://github.com/KingContaria/"
			}
		}
	],
	"contact": {
		"sources": "https://github.com/KingContaria/StandardSettings",
		"issues": "https://github.com/KingContaria/StandardSettings/issues"
	},
